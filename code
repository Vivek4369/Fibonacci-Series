#include <stdio.h>
int main()
{
   int a = 0, b = 1, c, n;
   printf("write fibonacci series upto :");
   scanf("%d", &n);
   if( n==0){
      printf("  ");
   }
   else if ( n == 1 ){
      printf("%d",a);
   }
   else if ( n == 2 ){
      printf("%d, %d", a,b);
   }
   else{
      printf("%d, %d",a,b);
   }
  while(1){
      c = a + b; 
      a = b;
      b = c;
      printf(" ,%d", c);
   }
   return 0;
} 
